{
  "name": "toto",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc -w --type-check",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:build": "node scripts/merge-prisma.ts && prisma format --schema src/config/database/schema.merged.prisma",
    "prisma:validate": "prisma validate --schema src/config/database/schema.merged.prisma",
    "prisma:generate": "prisma generate --schema src/config/database/schema.merged.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/config/database/schema.merged.prisma",
    "prisma:deploy": "prisma migrate deploy --schema src/config/database/schema.merged.prisma",
    "prisma:studio": "prisma studio --schema src/config/database/schema.merged.prisma",
    "prisma:seed": "tsx src/config/database/seeds/seeds.ts"
  },
  "dependencies": {
    "@fastify/formbody": "^8.0.2",
    "@fastify/secure-session": "^8.1.1",
    "@fastify/static": "^8.0.4",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.7",
    "@nestjs/platform-express": "^11.0.7",
    "@nestjs/platform-fastify": "^11.0.7",
    "@nestjs/swagger": "^11.0.3",
    "@prisma/client": "^6.14.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/randomstring": "^1.3.0",
    "argon2": "^0.44.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5.2.1",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.3",
    "randomstring": "^1.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
